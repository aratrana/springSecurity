@CHARSET "ISO-8859-1";
body {
	background: #ffffff;
	/* position: relative;
	padding: 200px; */
	background-image:url(../images/background.jpg);
	background-size:100% 1000px;
	background-repeat:no-repeat;
	font-family: verdana;
}
#rcorners2 {
    border-radius: 8px;
    border: 2px solid #ffffff;
    margin-left:30%;
    margin-top:10%;
    width: 650px;
    height: 450px;  
    background-color: #ffffff;  
}
.row {
  width: 850px;
  margin: 30px auto;
}

.col {
  width: 250px;
  float: left;
}

.col.left {
  font-family: Arial, sans-serif;
  text-shadow: 1px 1px 0px #fff;
  padding-left: 30px;
  color: #333;
  padding-bottom: 10px;
}

.col.right {
  margin-left: 20px;
  padding-left: 30px;
  border-left: 1px solid #aaa;
}
